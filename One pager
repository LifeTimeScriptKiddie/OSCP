	    bash process monitor
	    	#!/bin/bash
		IFS=$'\n'
		old_process=$(ps -eo pid,user,command)
		while true; do
		  new_process=$(ps -eo pid,user,command)
		  diff <(echo "$old_process") <(echo "$new_process")
		  sleep 1
		  old_process=$new_process
		done


	    
